{
  "name": "gdocs-preview",
  "description": "Use the Google Doc viewer for files linked in GitHub issues",
  "version": "0.5",
  "manifest_version": 2,
  "content_scripts": [{
    "js": ["contentscript.js"],
    "matches": [
      "*://*/*"
    ],
    "all_frames": true
  }],
  "web_accessible_resources": [
    "frame.html",
    "script.js"
  ]
}
